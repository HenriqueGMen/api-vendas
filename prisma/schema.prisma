generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id String @id @default(uuid())
  name String
  price Float
  quantity Int
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}